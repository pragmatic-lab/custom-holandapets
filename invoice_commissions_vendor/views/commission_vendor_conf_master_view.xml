<?xml version="1.0" encoding="utf-8"?>
<odoo>

	<record id="commission_vendor_conf_master_view_form" model="ir.ui.view">
		<field name="name">commission.vendor.conf.master.form</field>
		<field name="model">commission.vendor.conf.master</field>
		<field name="arch" type="xml">
			<form>

				<h3 class="panel-title">Configuración Comisión</h3>

				<group>
					<field name="name"/>
				</group>

				<group string="Comisión">
					<field name="commission_vendor_conf_ids" nolabel="1" />
				</group>

			</form>
		</field>
	</record>

	<record id="commission_vendor_conf_master_view_tree" model="ir.ui.view">
		<field name="name">commission.vendor.conf.master.tree</field>
		<field name="model">commission.vendor.conf.master</field>
		<field name="arch" type="xml">
			<tree>
				<field name="name"/>
			</tree>
		</field>
	</record>

		
</odoo>