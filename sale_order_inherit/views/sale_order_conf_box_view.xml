	<record id="sale_order_conf_box_view_form" model="ir.ui.view">
		<field name="name">sale.order_conf_box.form</field>
		<field name="model">sale.order_conf_box</field>
		<field name="arch" type="xml">
			<form>

				<group string="Datos Principales">
					<group col='2'>
						<field name="serial"/>
			
					</group>
					<group col='2'>
					
						<field name="name"/>
					</group>
				</group>
				<group string="Descripci贸n">
					<field name="description" nolabel="1"/>
				</group>
		
			</form>
		</field>
	</record>
		
		
	<record id="sale_order_conf_box_view_tree" model="ir.ui.view">
		<field name="name">sale.order_conf_box.tree</field>
		<field name="model">sale.order_conf_box</field>
		<field name="arch" type="xml">
			<tree>
				<field name="serial"/>
				<field name="name"/>
			</tree>
		</field>
	</record>


